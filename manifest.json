{
    "manifest_version": 2,
    "name": "Go Study",
    "version": "1.0",
    "description": "press thwe button and study",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "content_scripts":  [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon16.png"
    }
}